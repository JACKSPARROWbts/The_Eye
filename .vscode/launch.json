{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Chrome disable-web-security",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}",
        "runtimeArgs": [
          "--disable-web-security",
          "--user-data-dir=~/ChromeUserData/"
        ]
      }
    ]
  }